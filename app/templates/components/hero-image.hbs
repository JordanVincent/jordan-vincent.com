<div class="top-nav hidden-xs">
  <div class="container">
    {{nav-inline}}
  </div>
</div>

<header style={{headerStyle}}>
  {{#if title}}
    <div class="container">
      <h1 style={{headerTitleStyle}}>{{title}}</h1>
    </div>
  {{/if}}
</header>

<div class="sub-header" style={{subHeaderStyle}}>
  <div class="container">
    {{yield}}
  </div>
</div>

<div class="header-bottom">
  {{ragged-edge}}
</div>